<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
       <shape android:shape="rectangle">
           <solid android:color="#ffffff"/>
		    <corners
		        android:radius="5dp"/>
		    <stroke 
		        android:width="1dp" 
		        android:color="#8bb80e"/>
	   </shape>  
    </item>
    
    <item>
       <shape android:shape="rectangle">
            <solid android:color="#0057ded7"/>
		    <corners
		        android:radius="5dp"/>
		    <stroke 
		        android:width="1dp" 
		        android:color="#8bb80e"/>
	   </shape>  
    </item>
    
</selector>










   