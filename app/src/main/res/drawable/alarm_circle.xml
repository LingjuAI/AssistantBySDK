<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape
            android:innerRadius="130dp"
            android:shape="ring"
            android:thickness="0dp"
            android:useLevel="false" >
            <stroke
                android:width="2dp"
                android:color="#f09c42" />
        </shape>
    </item>
    <item
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp">
        <shape
            android:innerRadius="0dp"
            android:shape="ring"
            android:thickness="115dp"
            android:useLevel="false" >
            <solid android:color="@color/base_blue" />
        </shape>
    </item>

</layer-list>